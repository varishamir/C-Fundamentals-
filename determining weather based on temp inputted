#include <iostream>
using namespace std;
int main()
{
	int temperature;
	cout<<"enter temperature= ";
	cin>>temperature;
 
	if(temperature>30)
	{ cout<<"The temperature is hot";
	}
	
	else if((temperature<=30)&&(temperature>=15))
	{ cout<<"The temperature is Normal";
	}
	
	else
	{ cout<<"The temperature is Cold";
	}
}
