include <iostream>
using namespace std;
int main()
{
	int marks;
	cout<<"enter your marks= ";
	cin>>marks;
 
	if(marks>=80&&marks<=100)
	{ cout<<"Grade: A";
	}
	
	else if(marks>=60&&marks<=79)
	{ cout<<"Grade: B";
	}
	
	else if(marks>=50&&marks<=59)
	{ cout<<"Grade: C";
	}
	
	else
	{ cout<<"Fail";
	}
}
