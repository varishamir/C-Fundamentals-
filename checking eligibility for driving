#include <iostream>
#include <string>
using namespace std;
int main()
{
	int age;
	cout<<"enter your age= ";
	cin>>age;
 
	if(age>=18)
	{ std::string q;
	  cout<<"Do you have a Driving Licence(Yes/No)? ";
	  cin>>q;
	  
	  if( q=="Yes"|| q=="YEs"|| q=="YES" || q=="yES"||q=="yeS"|| q=="yes"||q=="yEs"||q=="YeS"||q=="YEs")
	  { cout<<"You are eligible to drive!";
	  }
	  
	  else
	  { cout<<"You are not eligible to drive";
	  }
	}
	
	else
	{ cout<<"You are not eligible to drive";
	}
}
